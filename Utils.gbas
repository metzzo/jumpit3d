FUNCTION LoadModel%: Path$
	LOCAL id% = GENX_OBJ()
	X_LOADOBJ "model/"+Path$, id
	IF X_NUMFACES(id) = 0
		DEBUG "Model "+Path$+" does not exist \n"
	ENDIF
	
	RETURN id
ENDFUNCTION



